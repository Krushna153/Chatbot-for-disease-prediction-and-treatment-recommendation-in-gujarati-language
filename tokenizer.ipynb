    "from utils.stopwords import stopwords\n",
    "import re\n",
    "\n",
    "def SentenceTokenizer(data):\n",
    "    data = data.strip()\n",
    "    data = re.sub(r'([.!?])', r'\\1 ', data)\n",
    "    data = re.split(r'  ',data)\n",
    "    if not data[-1]:\n",
    "        del(data[-1])\n",
    "    return data"
